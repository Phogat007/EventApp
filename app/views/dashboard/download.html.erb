<div class="recent_events">
  <p class="heading">Download events</p>
  <div class="event-table">
    <% @events.each do |event| %>
      <div class="event-row">
        <%= link_to event.name, event_path(event), class: "event-name" %>
        <%= link_to 'Download Event Details', download_event_path(event), class: "download-link" %>
      </div>
    <% end %>
  </div>
</div>